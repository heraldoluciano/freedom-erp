	<queryString>
		<![CDATA[select pd.codprod, pd.descprod, et.numeroft, et.dtrevisaoft
, et.desccompl, et.finalidadeuso, et.modousar, et.mecanismoacao
 ,img01.binimg img01
 ,img02.binimg img02 ,img03.binimg img03
 ,img04.binimg img04, img05.binimg img05
, img06.binimg img06
 from eqproduto pd
 inner join ppestrutura et on et.codemp=pd.codemp
 and et.codfilial=pd.codfilial and et.codprod=pd.codprod
 left outer join sgprefere5 p5 on p5.codemp=7 and p5.codfilial=1
 left outer join sgimagem img01 on img01.codemp=p5.codempi1
 and img01.codfilial=p5.codfiliali1 and img01.codimg=p5.codimgft01
 left outer join sgimagem img02 on img02.codemp=p5.codempi2
 and img02.codfilial=p5.codfiliali2 and img02.codimg=p5.codimgft02
 left outer join sgimagem img03 on img03.codemp=p5.codempi3
 and img03.codfilial=p5.codfiliali3 and img03.codimg=p5.codimgft03
 left outer join sgimagem img04 on img04.codemp=p5.codempi4
 and img04.codfilial=p5.codfiliali4 and img04.codimg=p5.codimgft04
 left outer join sgimagem img05 on img05.codemp=p5.codempi5
 and img05.codfilial=p5.codfiliali5 and img05.codimg=p5.codimgft05
 left outer join sgimagem img06 on img06.codemp=p5.codempi6
 and img06.codfilial=p5.codfiliali6 and img06.codimg=p5.codimgft06
  where pd.codemp=7 and pd.codfilial=1 and pd.codprod=715 and et.seqest=1]]>
	</queryString>
