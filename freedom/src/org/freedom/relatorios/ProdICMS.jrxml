	<queryString>
		<![CDATA[select pd.codprod, pd.descprod, cf.codncm,  icf.siglauf, tfc.codfisccli, tfc.descfisccli, icf.margemvlagr, icf.aliqfiscintra, icf.aliqfisc
from eqproduto pd
inner join lfclfiscal cf
on cf.codemp=pd.codempfc and cf.codfilial=pd.codfilialfc and cf.codfisc=pd.codfisc
inner join lfitclfiscal icf
on icf.codemp=cf.codemp and icf.codfilial=cf.codfilial and icf.codfisc=cf.codfisc
inner join lftipofisccli tfc
on tfc.codemp=icf.codempfc and tfc.codfilial=icf.codfilialfc and tfc.codfisccli=icf.codfisccli
where pd.codemp=7 and pd.codfilial=1
and icf.tipofisc='FF'
and icf.ativoitfisc='S'
order by pd.descprod, pd.codprod, icf.siglauf, tfc.codfisccli]]>
	</queryString>
